{
    "Name": "Get Event Listeners",
    "Description": "Get all the event listeners on the page",
    
    "Category": "Tricks",
    "Authors": ["@pankajparashar"],
    
    "Chrome": {
        "Code":
            '''
            $$("*").map(el => {
                return { el, listeners: getEventListeners(el) };
            }).filter(data => {
                return Object.keys(data.listeners).length;
            });
            '''
        }
}